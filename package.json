{
  "name": "sudoku-kata",
  "version": "1.0.0",
  "description": "",
  "module": "src/index.ts",
  "type": "module",
  "author": "gustaveWPM (github.com/gustaveWPM)",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "make": "bun install && webpack",
    "clean": "rm -rf dist",
    "fclean": "bun clean && rm -rf node_modules",
    "re": "bun fclean && bun install && bun make",
    "start": "bun run dist/bundle.js",
    "dev": "bun --watch run src/index.ts",
    "ts-node": "bun run src/index.ts",
    "commit": "git-cz",
    "format": "prettier --log-level warn --write .",
    "ts-prune": "ts-prune | .ts-prune/sanitize.py --quiet",
    "ts-prune-verbose": "ts-prune | .ts-prune/sanitize.py",
    "lint": "eslint \"**/*.{js,ts}\" --max-warnings 0",
    "test": "bun test",
    "coverage": "bun test --coverage"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^18.1.0",
    "@commitlint/cz-commitlint": "^18.2.0",
    "@swc/core": "^1.3.100",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "bun-types": "latest",
    "commitizen": "^4.3.0",
    "editorconfig": "^2.0.0",
    "eslint": "8.47.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-only-error": "^1.0.2",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-mock": "^29.7.0",
    "lint-staged": "^15.0.2",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "swc": "^1.0.11",
    "swc-loader": "^0.2.3",
    "terser-webpack-plugin": "^5.3.9",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-prune": "^0.10.3",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpackbar": "^5.0.2"
  }
}
