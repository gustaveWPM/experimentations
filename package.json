{
  "name": "sudoku-kata",
  "version": "1.0.0",
  "description": "",
  "module": "src/main.ts",
  "type": "module",
  "author": "gustaveWPM (github.com/gustaveWPM)",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "bundle": "bun lint && bun build ./src/main.ts --outdir ./dist --minify",
    "make": "bun install && bun bundle",
    "clean": "rm -rf dist",
    "fclean": "bun clean && rm -rf node_modules",
    "re": "bun fclean && bun install && bun make",
    "start": "bun run dist/main.js",
    "dev": "bun --watch run src/main.ts",
    "ts-node": "bun run src/main.ts",
    "commit": "git-cz",
    "format": "prettier --log-level warn --write .",
    "ts-prune": "ts-prune | .ts-prune/sanitize.py --quiet",
    "ts-prune-verbose": "ts-prune | .ts-prune/sanitize.py",
    "lint": "bun run --bun eslint \"**/*.{js,ts}\" --max-warnings 0",
    "test": "bun test",
    "coverage": "bun test --coverage"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^18.1.0",
    "@commitlint/cz-commitlint": "^18.2.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "bun-types": "latest",
    "commitizen": "^4.3.0",
    "commitlint": "^18.4.3",
    "editorconfig": "^2.0.0",
    "eslint": "8.47.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-only-error": "^1.0.2",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-mock": "^29.7.0",
    "lint-staged": "^15.0.2",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-prune": "^0.10.3",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
